<meta charset="{{ site.charset }}"/>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="profile" href="http://gmpg.org/xfn/11">
<title>{{ seo.title }}{{ seo.title_template ? ' | ' ~ seo.title_template : null }}</title>

<meta name="description" content="{{ seo.desc }}">
<meta property="og:locale" content="{{ seo.lang }}"/>
<meta property="og:type" content="{{ seo.og_type }}" />
<meta property="og:title" content="{{ seo.title }}" />
<meta property="og:description" content="{{ seo.desc }}" />
<meta property="og:url" content="{{ seo.url }}"/>
<meta property="og:site_name" content="{{ seo.site_name }}"/>
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="630">
<meta property="og:image" content="{{ seo.thumbnail }}" />
<meta property="og:image:alt" content="{{ seo.desc }}" />
<meta name="twitter:card" content="summary_large_image"/>
<meta name="twitter:description" content="{{ seo.desc }}"/>
<meta name="twitter:title" content="{{ seo.title }}"/>
<meta name="twitter:image" content="{{ seo.thumbnail }}"/>
<meta itemprop="name" content="{{ seo.title }}">
<meta itemprop="description" content="{{ seo.desc }}">
<meta itemprop="image" content="{{ seo.thumbnail }}">

{% if not blog_public %}
    <meta name="googlebot" content="noindex, nofollow">
{% endif %}

